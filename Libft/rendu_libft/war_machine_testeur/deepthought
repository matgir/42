gcc -g3 -Wall -Werror -Wextra -c ft_itoa.c -o ft_itoa.o
gcc -g3 -Wall -Werror -Wextra -c ft_memchr.c -o ft_memchr.o
gcc -g3 -Wall -Werror -Wextra -c ft_memcmp.c -o ft_memcmp.o
gcc -g3 -Wall -Werror -Wextra -c ft_memcpy.c -o ft_memcpy.o
gcc -g3 -Wall -Werror -Wextra -c ft_memmove.c -o ft_memmove.o
gcc -g3 -Wall -Werror -Wextra -c ft_memset.c -o ft_memset.o
gcc -g3 -Wall -Werror -Wextra -c ft_split.c -o ft_split.o
gcc -g3 -Wall -Werror -Wextra -c ft_strchr.c -o ft_strchr.o
gcc -g3 -Wall -Werror -Wextra -c ft_strdup.c -o ft_strdup.o
gcc -g3 -Wall -Werror -Wextra -c ft_strjoin.c -o ft_strjoin.o
gcc -g3 -Wall -Werror -Wextra -c ft_strlcat.c -o ft_strlcat.o
